<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find robot)/urdf/parts/shin/shin.gazebo"/>
  
  <xacro:macro name="shin_v0" params="prefix parent *joint_origin *joint_axis">
	<joint name="${prefix}_shin_joint" type="revolute">
	  <xacro:insert_block name="joint_origin"/>
	  <parent link="${parent}"/>
	  <child link="${prefix}_shin_link"/>
		<limit effort="500" velocity="10" lower="-3" upper = "3"/>
       <dynamics damping="0.01" friction="100"/>
	  <xacro:insert_block name="joint_axis"/>
	</joint>

	<link name="${prefix}_shin_link">
	  <visual>
		<geometry>
		  <mesh filename="/home/alex/Documentos/universidad/robot/colcon_ws/src/robot/meshes/shin.dae"/>
		</geometry>
	  </visual>
	  <collision>
	  		 <!-- <origin
			xyz="-0.1825 0.012 0"
			rpy="0 1.57075 0" /> -->
			<origin xyz="0 0 0" rpy="1.570796 0 0"/>

		<geometry>
		  <mesh filename="/home/alex/Documentos/universidad/robot/colcon_ws/src/robot/meshes/shin.stl"/>
		  <!--<cylinder length="0.345" radius="0.025"/>-->
		</geometry>
	  </collision>
	  <inertial>
	  	
		<origin xyz="-0.158 0.011 0.0"/>
		<mass value="0.66"/>
		<inertia ixx="0.0003" ixy="0.0013" ixz="0.000045" iyy="0.029" iyz="-0.000003" izz="0.029" />
	  </inertial>
	</link>
  </xacro:macro>	
  
</robot>
