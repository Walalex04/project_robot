<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="shin_gazebo_v0" params="prefix">
    <gazebo reference="${prefix}_shin_link">
      <selfCollide>false</selfCollide>
      <mu1>0.8</mu1>
      <mu2>0.8</mu2>
      <minDepth>0</minDepth>
      <kp  value="1000.0"/>
      <kd  value="0.1"/>
    </gazebo>
  </xacro:macro>
</robot>

